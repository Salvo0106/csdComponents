{
    "name": "csdsolutions-csdjs",
    "version": "1.0.70",
    "description": "Libreria per i progetti di CSD Solutions",
    "main": "dist/csd-bundle.min.js",
    "module": "dist/csd-bundle.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist",
        "*.js"
    ],
    "scripts": {
        "version:patch": "npm version patch --no-git-tag-version",
        "build": "webpack --config webpack.config.js",
        "prepare": "npm run version:patch && npm run build",
        "start": "webpack serve --config webpack.config.js --mode development --open",
        "serve": "node server.js"
    },
    "keywords": [
        "csd",
        "solutions",
        "javascript",
        "library"
    ],
    "author": "CSD Solutions",
    "license": "ISC",
    "dependencies": {
        "chokidar": "^3.5.3",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@babel/core": "^7.26.9",
        "@babel/preset-env": "^7.26.9",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^13.0.0",
        "terser": "^5.27.2",
        "webpack": "^5.90.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.2"
    }
}
